{
  "name": "react-transport",
  "description": "Google Chrome extension for demo of react-transport.",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "https://mail.google.com/*",
        "https://inbox.google.com/*"
      ],
      "js": ["lib/inboxsdk.js", "js/content.js"],
      "css": ["styles/main.css"],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "https://mail.google.com/",
    "https://inbox.google.com/"
  ],
  "manifest_version": 2
}
